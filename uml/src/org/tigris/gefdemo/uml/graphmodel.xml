<graphmodel>
    <connections>
        <connection>
            <edge>org.tigris.gefdemo.uml.model.UmlAssociationClass</edge>
            <port>org.tigris.gefdemo.uml.model.UmlModelElement</port>
            <port>org.tigris.gefdemo.uml.model.UmlModelElement</port>
        </connection>
        <connection>
            <edge>org.tigris.gefdemo.uml.model.UmlAssociationEnd</edge>
            <port>org.tigris.gefdemo.uml.model.UmlClassifier</port>
            <port>org.tigris.gefdemo.uml.model.UmlAssociation</port>
        </connection>
        <connection>
            <edge>org.tigris.gefdemo.uml.model.UmlAssociationEnd</edge>
            <port>org.tigris.gefdemo.uml.model.UmlAssociation</port>
            <port>org.tigris.gefdemo.uml.model.UmlClassifier</port>
        </connection>
        <connection>
            <!-- Attempting to draw an association end from one classifier to another is trapped in the code
                 This results in the creation of an association in the model with 2 association ends
                 joining that new association to the 2 given classifiers --> 
            <edge>org.tigris.gefdemo.uml.model.UmlAssociationEnd</edge>
            <port>org.tigris.gefdemo.uml.model.UmlClassifier</port>
            <port>org.tigris.gefdemo.uml.model.UmlClassifier</port>
        </connection>
        <connection>
            <edge>org.tigris.gefdemo.uml.model.UmlDependency</edge>
            <port>org.tigris.gefdemo.uml.model.UmlModelElement</port>
            <port>org.tigris.gefdemo.uml.model.UmlModelElement</port>
        </connection>
        <connection>
            <edge>org.tigris.gefdemo.uml.model.UmlGeneralization</edge>
            <port>org.tigris.gefdemo.uml.model.UmlGeneralizableElement</port>
            <port>org.tigris.gefdemo.uml.model.UmlGeneralizableElement</port>
        </connection>
    </connections>
</graphmodel>
